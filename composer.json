{
  "name": "hurah/hurah-installer",
  "description" : "Installer scripts for Hurah!",
  "type": "composer-plugin",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "ext-mysqli": "*",
    "ext-json": "*",
    "composer/composer": "^2.0.4",
    "composer-plugin-api": "^1.1.0"
  },
  "autoload": {
    "psr-4": {
      "Hi\\": "src/Hi"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HiTest\\" : "test"
    }
  },
  "extra": {
    "class": "Hi\\Plugin"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3@dev"
  }
}
