{
	"name": "hurah/hurah-installer",
	"description": "Installer scripts for Hurah!",
	"type": "composer-plugin",
	"license": "MIT",
	"minimum-stability": "dev",
	"require": {
		"php": ">=7.4",
		"ext-json": "*",
		"composer/composer": "^2.0.8",
		"composer-plugin-api" : "*"
	},
	"autoload": {
		"psr-4": {
			"Hi\\": "src/Hi"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"HiTest\\": "test"
		}
	},
	"extra": {
		"class": "Hi\\Plugin"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.3@dev"
	}
}
